@using MVCKutuphane.Models.Entity
@model List<TBLMesajlar>
@{
    ViewBag.Title = "Giden";
    Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}

<h2>Gelen Mesajlar Sayfası</h2>
<br />

<table class="table table-bordered">
    <tr>
        <th>
            Alıcı
        </th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>İşlem</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ALICI</td>
            <td>@item.KONU</td>
            <td>@item.ICERIK</td>
            <td>@item.TARIH</td>
        </tr>
    }
</table>

